{
    "schemaVersion": 1,
    "id": "crafttweaker",
    "version": "${version}",
    "name": "CraftTweaker",
    "description": "Customize your minecraft experience!",
    "authors": [
        "Jaredlll08",
        "Kindlich",
        "StanHebben",
        "TheSilkMiner"
    ],
    "contact": {
        "homepage": "https://minecraft.curseforge.com/projects/crafttweaker",
        "sources": "https://github.com/CraftTweaker/CraftTweaker"
    },
    "license": "MIT",
    "icon": "assets/crafttweaker/logo.png",
    "environment": "*",
    "entrypoints": {
        "main": [
            "com.blamejared.crafttweaker.CraftTweakerFabric"
        ],
        "client": [
            "com.blamejared.crafttweaker.CraftTweakerFabricClient"
        ],
        "fabric-gametest": [
            "com.blamejared.crafttweaker.gametest.CraftTweakerGameTests"
        ],
        "rei": [
            "com.blamejared.crafttweaker.impl.compat.rei.CraftTweakerPlugin"
        ]
    },
    "mixins": [
        "crafttweaker.mixins.json",
        "crafttweaker.fabric.mixins.json"
    ],
    "depends": {
        "fabricloader": ">=0.13.1",
        "fabric": "*",
        "minecraft": "1.18.x",
        "java": ">=17",
        "ingredient-extension-api": "*"
    },
    "accessWidener": "crafttweaker.accesswidener"
}
  