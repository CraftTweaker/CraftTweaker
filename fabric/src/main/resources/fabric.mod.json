{
  "schemaVersion": 1,
  "id": "${MOD_ID}",
  "version": "${version}",
  "name": "${NAME}",
  "description": "${DESCRIPTION}",
  "authors": ["${AUTHOR}"],
  "contact": {
    "homepage": "${CURSE_HOMEPAGE_LINK}",
    "sources": "${GIT_REPO}",
    "issues": "${GIT_REPO}/issues"
  },
  "license": "MIT",
  "icon": "logo.png",
  "environment": "*",
  "entrypoints": {
    "main": [
      "com.blamejared.crafttweaker.CraftTweakerFabric"
    ],
    "client": [
      "com.blamejared.crafttweaker.CraftTweakerFabricClient"
    ],
    "fabric-gametest": [
      "com.blamejared.crafttweaker.gametest.CraftTweakerGameTests"
    ]
  },
  "mixins": [
    "crafttweaker.mixins.json",
    "crafttweaker.fabric.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=${FABRIC_LOADER}",
    "fabric": ">=${FABRIC}",
    "minecraft": "${MINECRAFT}",
    "java": ">=${JAVA}"
  },
  "accessWidener": "crafttweaker.accesswidener"
}
  